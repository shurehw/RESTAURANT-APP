# Supabase
NEXT_PUBLIC_SUPABASE_URL=https://xxxxx.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...
SUPABASE_SERVICE_ROLE_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...

# Toast (Delilah LA)
TOAST_API_KEY=your_toast_api_key
TOAST_RESTAURANT_GUID=your_restaurant_guid

# Square (Nice Guy LA)
SQUARE_ACCESS_TOKEN=your_square_access_token
SQUARE_LOCATION_ID=your_location_id

# OCR Service (example: Google Document AI)
GOOGLE_CLOUD_PROJECT_ID=your_project
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account.json

# R365 Integration
R365_WEBHOOK_URL=https://api.restaurant365.com/webhooks/ap_import

# TipSee POS (H.wood Group PostgreSQL)
TIPSEE_DB_HOST=TIPSEE_HOST_REDACTED
TIPSEE_DB_USER=your_tipsee_user
TIPSEE_DB_PASSWORD=your_tipsee_password
TIPSEE_DB_NAME=postgres
TIPSEE_DB_PORT=5432

# Prophet Forecaster (optional overrides)
MODEL_VERSION=prophet_v1_baseline
FORECAST_DAYS=42

# Anthropic (Claude AI for forecast explanations)
ANTHROPIC_API_KEY=sk-ant-...

# Application
NODE_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3000

# Sentry Error Monitoring
NEXT_PUBLIC_SENTRY_DSN=https://your-sentry-dsn@sentry.io/your-project-id
SENTRY_ORG=your-sentry-organization-slug
SENTRY_PROJECT=your-sentry-project-name
SENTRY_AUTH_TOKEN=your-sentry-auth-token
